{{#section 'js'}}
    <script type="text/javascript">
        function goHome() {
            window.location = '/home';
        }
        function showMess() {
            swal({
                type: 'error',
                title: 'Oops...',
                text: 'Chức năng này chưa được cài đặt'                
            })
        }
    </script>

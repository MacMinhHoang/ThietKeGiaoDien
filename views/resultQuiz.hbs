<div class="result-panel">
    <img class="img-panel" src="/images/Boders/Rank_Score.png">
    <div class="text-player"><label>{{session.user}}</label></div>
    <img class="img-score" src="/images/Icons/scores.png"> 
    <div class="text-score"><label>{{session.score}} điểm</label></div>
    <img class="img-rank" src="/images/Icons/rank.png"> 
    <div class="text-rank"><label>Hạng {{rank}}</label></div>
</div>
<input type="image" src="/images/Icons/back.png" width="100px" height="100px" onclick="goHome()" 
    title="Trang Chủ">
<div style="text-align:center; margin-top: 400px;">
    <input type="image" src="https://i.imgur.com/7DArY2w.png" title="Đóng góp câu hỏi" onclick="showMess()">
